{
  "name": "near-willem-workspaces-ava",
  "version": "1.0.0",
  "description": "Thin wrapper around near-willem-workspaces to make it easier to use with AVA and TypeScript",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/willemneal/workspaces-js.git",
    "directory": "packages/ava"
  },
  "author": "Near Inc <team@near.org>",
  "license": "(MIT AND Apache-2.0)",
  "main": "dist/index.js",
  "bin": {
    "near-willem-workspaces-ava": "scripts/cli.js"
  },
